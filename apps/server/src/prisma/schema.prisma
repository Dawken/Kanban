generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(cuid())
  login    String  @unique
  password String
  name     String
  lastName String
  Board    Board[]
}

model Board {
  id        String @id @default(cuid())
  boardName String
  user      User   @relation(fields: [userId], references: [id])
  userId    String
}
